// Java
import java.util.*;

class lab1 {
    public static int[] twoSum(int[] nums, int target)
    {
        int[] answer = null;
        Set<Integer> map = new HashSet<>();
        for(int i = 0; i < nums.length; i++)
        {
            int difference = target - nums[i];
            if(map.contains(difference))
            {
                answer = new int[2];
                answer[0] = nums[i];
                answer[1] = difference;
                break;
            }
            map.add(nums[i]);
        }
        return answer;
    }

    public static void main(String[] args)
    {
        //Prints out the actual numbers being added
        //that add up to the target
        System.out.println(Arrays.toString(twoSum(new int[]{1, 2, 3, 4, 5, 6, 7}, 9)));
    }
}

# Python
def twosum(nums, target):
    length = len(nums)
    for i in range(length):
        for j in range(i + 1, length):
            if nums[i] + nums[j] == target:
                #Returns the first match to target
                return [i, j]

n = [1, 2, 3, 4, 5, 6, 7]
t = 9
#Prints out the location of the two numbers
print(twosum(n, t))
